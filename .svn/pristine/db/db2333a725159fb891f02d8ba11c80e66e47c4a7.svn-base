    Private Sub UltraButton1_Click(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles UltraButton1.Click
        Dim cmd As New SqlClient.SqlCommand("", cnn)
        cnn.Open()
        Select Case Split(tOrd.Text, "-").Length
            Case 3
                cmd.CommandText = "delete from ord_comm_override where ord_comm_override_ord_serial=" + Split(tOrd.Text, "-")(0) + " and ord_comm_override_ord_rev=" + Split(tOrd.Text, "-")(2) + " and ord_comm_override_ord_type='O'"
                cmd.ExecuteNonQuery()
                cmd.CommandText = "insert into ord_comm_override (ord_comm_override_ord_serial,ord_comm_override_ord_rev,ord_comm_override_ord_type,ord_comm_override_user,ord_comm_override_date,ord_comm_override_isr,ord_comm_override_osr,ord_comm_override_manager,ord_comm_override_supervisor) values (" + Split(tOrd.Text, "-")(0) + "," + Split(tOrd.Text, "-")(2) + ",'O','" + User + "','" + CStr(Now) + "'," + CStr(tISR.Value) + "," + CStr(tOSR.Value) + "," + CStr(tManager.Value) + "," + CStr(tSupervisor.Value) + ")"
                cmd.ExecuteNonQuery()
                MsgBox("Sales Commission Set", MsgBoxStyle.Information, "Complete")
            Case Else
                MsgBox("Must be an Order (ie 123456-0-0)", MsgBoxStyle.Information, "Number?")
        End Select
        cnn.Close()
        tOrd.Focus()
        tOrd.SelectAll()

    End Sub

    Private Sub tOrd_ValueChanged(ByVal sender As System.Object, ByVal e As System.EventArgs) Handles tOrd.ValueChanged

    End Sub

    Private Sub tOrd_Leave(ByVal sender As Object, ByVal e As System.EventArgs) Handles tOrd.Leave
        If Split(tOrd.Text, "-").Length <> 3 Then
            MsgBox("Must be an Order (ie 123456-0-0)", MsgBoxStyle.Information, "Number?")
            Exit Sub
        End If
        Dim cmd As New SqlClient.SqlCommand("select * from ord_comm_override where ord_comm_override_ord_serial=" + Split(tOrd.Text, "-")(0) + " and ord_comm_override_ord_rev=" + Split(tOrd.Text, "-")(2) + " and ord_comm_override_ord_type='O'", cnn)
        Dim dr As SqlClient.SqlDataReader

        cnn.Open()
        dr = cmd.ExecuteReader
        If dr.HasRows Then
            dr.Read()
            tISR.Value = dr("ord_comm_override_isr")
            tOSR.Value = dr("ord_comm_override_osr")
            tManager.Value = dr("ord_comm_override_manager")
            tSupervisor.Value = dr("ord_comm_override_supervisor")
        Else
            tISR.Value = 0
            tOSR.Value = 0
            tManager.Value = 0
            tSupervisor.Value = 0
        End If
        cnn.Close()
    End Sub
